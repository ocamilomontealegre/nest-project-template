{
  "name": "nest_project_template",
  "version": "0.0.1",
  "description": "",
  "author": "@ocamilomontealegre",
  "private": true,
  "license": "UNLICENSED",
  "keywords": [
    "typescript",
    "nest",
    "template"
  ],
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:verbose": "eslint \"{src,apps,libs,test}/**/*.ts\" --debug",
    "check:ts": "npx tsc --noEmit",
    "check:verbose": "npx tsc --noEmit --diagnostics",
    "test:unit": "vitest --config test/config/vitest.config.unit.ts --coverage --no-watch",
    "test:e2e": "vitest --config test/config/vitest.config.e2e.ts --coverage --no-watch --silent",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test": "npm-run-all --parallel test:unit test:e2e",
    "prepare": "husky"
  },
  "dependencies": {
    "@cspell/eslint-plugin": "^8.15.7",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.0",
    "@swc/cli": "0.4.1-nightly.20240914",
    "@swc/core": "^1.7.26",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "globals": "^15.11.0",
    "nest-winston": "^1.9.7",
    "npm-run-all": "4.1.5",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typescript-eslint": "8.12.2",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.1.4",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.16.5",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}